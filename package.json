{
  "name": "memochat-backend",
  "version": "2.1.0",
  "description": "Enhanced Backend server for MemoChat - Voice & Screen Share application with authentication, volume controls, password protection, and spam protection",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "build": "echo 'No build step required'",
    "docker:build": "docker build -t memochat-backend .",
    "docker:run": "docker run -p 3001:3001 memochat-backend"
  },
  "keywords": [
    "voice-chat",
    "webrtc",
    "socket.io",
    "screen-share",
    "real-time",
    "memo",
    "chat",
    "authentication",
    "jwt",
    "login",
    "password-protection",
    "volume-control",
    "spam-protection",
    "bcrypt"
  ],
  "author": "MemoChat Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/izzethannn/memochat-backend.git"
  },
  "bugs": {
    "url": "https://github.com/izzethannn/memochat-backend/issues"
  },
  "homepage": "https://github.com/izzethannn/memochat-backend#readme"
}
